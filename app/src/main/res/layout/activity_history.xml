<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.HistoryActivity" android:background="#141414">

    <Button
            android:text="Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/backHistoryButton"
            android:onClick="backMainActivity" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.049"
            app:layout_constraintVertical_bias="0.023" android:textColor="#EEEEEE"/>
    <TextView
            android:text="Check Out History"
            android:layout_width="247dp"
            android:layout_height="65dp" android:id="@+id/textView2"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.024"
            android:textAlignment="center" android:textSize="24sp" android:textStyle="bold"
            android:fontFamily="serif" android:typeface="normal" android:textColor="#EEEEEE"/>
    <ListView
            android:layout_width="409dp"
            android:layout_height="651dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0" android:id="@+id/listViewHistory"/>
</androidx.constraintlayout.widget.ConstraintLayout>